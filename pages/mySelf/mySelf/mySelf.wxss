.nameBackView {
  width: 100%;
  height: 360rpx;
  border-bottom: 1px solid #bbb;
}
.oimageView {
  width: 100%;
  height: 360rpx;
  display: -webkit-flex;
  flex-flow: column;
  justify-content: center;
  align-items:center;
  background-image: url('http://img06.tooopen.com/images/20160818/tooopen_sy_175866434296.jpg');
  background-repeat:no-repeat;
  background-size:100% 100%;
  -moz-background-size:100% 100%;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  z-index: 20;
}
.userinfo-nickname {
  color: #aaa;
  z-index: 20;
}
.splitLine {
  height: 2px;
  background-color: #999;
  margin: 13rpx 3rpx;
}
.splitLine_tab {
  margin: 5rpx 20rpx;
  height: 2px;
  background-color: #ddd;
}